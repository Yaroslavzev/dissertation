@article{sis1_Pfitzner:se0205,

author = {Pfitzner, A. and Evain, M. and Petricek, V.},

title = {Cu\textsubscript{12}Sb\textsubscript{4}S\textsubscript{13}: A Temperature-Dependent Structure Investigation},
journal = {Acta Crystallographica Section B},
year = {1997},
volume = {53},
number = {3},
pages = {337--345},
month = {Jun},
doi = {10.1107/S0108768196014024},
url = {https://doi.org/10.1107/S0108768196014024},
abstract = {Synthetic tetrahedrite, Cu${\sb 12}$Sb${\sb 4}$S${\sb 13}$, obtained by reaction of the elements, has been investigated at various temperatures in the 295{--}573K range. It crystallizes in the cubic system with {\it a} = 10.3293(6){\AA}, {\it V} = 1102.1(2){\AA}${\sp 3}$, space group {\it I}\overline 43{\it m} and {\it Z} = 2 at room temperature. The structure refinement converged to a residue of {\it R} = 0.0165 (at room temperature, {\it wR} = 0.0200) for 389 independent reflections and 34 refined parameters. A Gram{--}Charlier non-harmonic development of the atomic displacement factor for both independent Cu atoms was used. The results show a disorder for the three-coordinated Cu atom, within and perpendicular to the plane of the three surrounding S atoms. However, although the non-harmonic probability density deformation increases with raising the temperature within this plane, it barely changes in the perpendicular direction. This suggests two different types of disorder: static in-plane and dynamic out-of-plane, therefore underlining a possible diffusion pathway for copper ions. To check the significance of the observed effects, the errors for the one-particle potentials and the probability density function maps were calculated by means of a Monte-Carlo method.},
language      = {english},
}


	
@article{sis1_1347-4065-8-4-443,
  
author={Nakanishi, Hisayuki and Endo, Saburo  and Irie, Taizo },
 
 title={On the Electrical and Thermal Properties of the Ternary Chalcogenides A$_2^I$B$^{IV}$X$_3$, A$^I$B$^{V}$X$_2$ and A$_3^I$B$^{V}$X$_4$ A$^I$~$=$~Cu; B$^{IV}=$~Ge, Sn; B$^{V}=$~Sb; X~$=$~S, Se, Te) II. Electrical and Thermal Properties of Cu\textsubscript{3}SbSe\textsubscript{4}},
  
journal={Japanese Journal of Applied Physics},
  volume={8},
  number={4},
  pages={443},
  url={http://stacks.iop.org/1347-4065/8/i=4/a=443},
  year={1969},
  abstract={Electrical and thermal conductivity and Hall, Seebeck and transverse Nernst-Ettings-hansen effects have been measured over the temperature range from 100°K to 500°K on Cu 3 SbSe 4 . From the temperature dependence of the Hall coefficient, the electron to hole mobility ratio is estimated to be about three. Temperature dependences of the resistivity and the Hall coefficient give an estimate of 0.13 eV for the energy gap at 0°K. The infrared transmission spectra at room temperature also showed a threshold of absorption at about 0.11 eV of photon energy. From the Seebeck coefficient data, the effective mass of holes is calculated to be 0.73 m 0 . The scattering parameter is determined to be zero over the temperature range studied which is the value expected for acoustic mode lattice scattering. The lattice thermal conductivity varies with temperature as T -1 . The dimensionless thermoelectric figure of merit, ZT, is evaluated as a function of temperature and the reduced Fermi level.},
  language      = {english},
}
	

